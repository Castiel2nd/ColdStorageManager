<UserControl x:Class="ColdStorageManager.Controls.RegControlTableBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ColdStorageManager.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
            <StackPanel Name="TableNamesSP" DockPanel.Dock="Top" Orientation="Horizontal" Margin="10">
					<StackPanel.Resources>
                        <ResourceDictionary>
                            <Style TargetType="Button">
                                <Setter Property="FontSize" Value="19"></Setter>
	                            <Setter Property="Margin" Value="3,5"></Setter>
                                <Setter Property="Padding" Value="3,5"></Setter>
                            </Style>
                        </ResourceDictionary>
					</StackPanel.Resources>
                <Button Click="TableName_OnClick"></Button>
            </StackPanel>
			<StackPanel Name="TableDisplayArea"></StackPanel>
    </DockPanel>
</UserControl>
